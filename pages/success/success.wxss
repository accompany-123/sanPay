.success{
    height: 100%;
    background: #fff;
}
.success-pay{
   background: #f9f8f9;
   text-align: center;
   padding: 50rpx 0;
}
.success-pay image{
    width: 100rpx;
    height: 100rpx;
    margin-top: 60rpx;
}
.success-pay .goHome{
    margin-top: 100rpx;
    background: #feaf1a;
    color: #fff;
    height: 80rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 230rpx;
    margin: 80rpx auto 0;
    border-radius: 10rpx;
}
.success-like{
    background: #fff;
}
.success-like .like-title{
    text-align: center;
    height: 90rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    border-bottom: 2px solid #f5f5f5;
}
.success-like .like-title text{
    color: #c00;
    font-size: 80rpx;
    margin: -10px 10rpx 0;
}
.like{
    padding: 20rpx;
}
.like image{
    width: 100%;
    height: 350rpx;
}
.like view{
    color: #999;
    font-size: 34rpx;
}
.like .price{
    color: #c00;
    font-size: 34rpx;
}
.like .num{
    display: flex;
    justify-content: space-between;
}
.like .num .button{
    background: #fe303e;
    color: #fff;
    height: 60rpx;
    font-size: 28rpx;
    width: 150rpx;
    border-radius: 10rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}